/**index.wxss**/
.container{
  height: 100vh;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  background-color: #ebebeb;
}

.content{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 200rpx;
}

#add{
  height: 100rpx;
  width: 200rpx;
  border-radius: 20rpx;
  background-color: rgb(31, 31, 255);
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;

}

#register{
  height: 100rpx;
  width: 250rpx;
  border-radius: 20rpx;
  background-color: rgb(250, 21, 238);
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 40rpx;

}

#search{
  height: 300rpx;
  width: 300rpx;
  border-radius: 50%;
  background-color: rgb(255, 102, 31);
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 60rpx;

}